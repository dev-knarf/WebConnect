<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebConnect.Data.Model"
                   namespace="WebConnect.Data.Model.Reservas">
  <class name="Dependencia" table="RESERVAS_DEPENDENCIAS" lazy="false" >
    <id name="ObjectId" type="Int32" unsaved-value="0">
      <column name="RECORD_ID" />
      <generator class="sequence">
        <param name="sequence" >DEPENDENCIA_SEC</param>
      </generator>
    </id>
    <property name="Nombre" type="String">
      <column name="NOMBRE" not-null="true" length="200" />
    </property>
    <!--<component name="Clase" class="WebConnect.Data.Model.Reservas.Compuestos.ClaseDependencia">
      <property name="ObjectId" type="Int32">
        <column name="RECORD_ID" not-null="true" />
      </property>
      <property name="Nombre" type="String">
        <column name="NOMBRE" not-null="true" length="200" />
      </property>
    </component>
    <component name="Estado" class="WebConnect.Data.Model.Reservas.Compuestos.Estado">
      <property name="ObjectId" type="Int32">
        <column name="RECORD_ID" not-null="true" />
      </property>
      <property name="Nombre" type="String">
        <column name="NOMBRE" not-null="true" length="200" />
      </property>
    </component>-->
    <property name="CreationDate" type="DateTime" insert="false" update="false">
      <column name="FECHA_CREACION" not-null="true" />
    </property>
    <join table="RESERVAS_TIPO" fetch="join">
      <key column="RECORD_ID" foreign-key="RECORD_ID" />
      <component name="Tipo" class="WebConnect.Data.Model.Reservas.Compuestos.TipoDependencia">
        <property name="ObjectId" type="Int32">
          <column name="RECORD_ID" not-null="true" />
        </property>
        <property name="Nombre" type="String">
          <column name="NOMBRE" not-null="true" length="200" />
        </property>
      </component>  
    </join>
  </class>
</hibernate-mapping>