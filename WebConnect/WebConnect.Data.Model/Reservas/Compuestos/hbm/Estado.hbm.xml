<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="WebConnect.Data.Model"
                   namespace="WebConnect.Data.Model.Reservas.Compuestos">
  <class name="Estado" table="RESERVAS_ESTADOS" lazy="false" >
    <id name="ObjectId" type="Int32" unsaved-value="0">
      <column name="RECORD_ID" />
      <generator class="sequence">
        <param name="sequence" >ESTADOS_SEC</param>
      </generator>
    </id>
    <property name="Nombre" type="String">
      <column name="NOMBRE" not-null="true" length="200" />
    </property>
  </class>
</hibernate-mapping>